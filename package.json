{
  "name": "element-plus-plus",
  "displayName": "Element Plus Plus",
  "description": "Enhance your FE development experience, especially with Element UI/Plus.",
  "version": "2.0.0",
  "engines": {
    "vscode": "^1.65.0"
  },
  "publisher": "microkof",
  "icon": "assets/logo.png",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microkof/vscode-element-plus-plus.git"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "element-plus-plus.browseSvg",
        "title": "Browse SVG"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "element-plus-plus.browseSvg",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {},
  "dependencies": {
    "fs-extra": "^10.0.1",
    "readdirp": "^3.6.0"
  }
}
